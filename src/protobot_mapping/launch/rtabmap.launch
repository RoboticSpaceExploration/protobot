<launch>
	<include file="$(find rtabmap_ros)/launch/rtabmap.launch">
	
    	<!-- If camera is used -->
    	<arg name="rgb_topic"         value="/depth_camera/image_raw" />
    	<arg name="camera_info_topic" value="/depth_camera/camera_info" />
    	<arg name="depth_topic"       value="/camera/depth/image_raw" />
    	<arg name="approx_rgbd_sync"  value="false" />
	
	<arg name="visual_odometry" value="false"/>
        <arg name="odom_topic" value="/localization/filtered/odom_local"/>
	
	</include>
</launch>
